{
    "name": "tii/laravel-telegram-bot",
    "description": "Integrates PHP Telegram Bot into Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Tii",
            "email": "mail@tii.one"
        }
    ],
    "homepage": "https://github.com/tii/laravel-telegram-bot",
    "keywords": ["laravel", "telegram", "bot"],
    "require": {
        "illuminate/support": "~7|~8|~9",
        "longman/telegram-bot": "^0.74.0",
        "ext-pcntl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "symfony/process": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Tii\\LaravelTelegramBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tii\\LaravelTelegramBot\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Tii\\LaravelTelegramBot\\TelegramBotServiceProvider"
            ],
            "aliases": {
                "CallbackButton": "Tii\\LaravelTelegramBot\\Facades\\CallbackButton",
                "Bot": "Tii\\LaravelTelegramBot\\Facades\\Bot"
            }
        }
    },
    "bin": [
        "bin/expose"
    ]
}
